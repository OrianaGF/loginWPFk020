<Window x:Class="_15022022.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_15022022"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel Orientation="Vertical" Vertical>
        <Label Name="etiqueta1" Margin="10" FontSize="20">
            Username: 
        </Label>
        <TextBox Name="usernameTextBox" VerticalAlignment="Center"></TextBox>
        <Label Margin="10" FontSize="20">Password:</Label>
        <PasswordBox Name="passwordTextbox" VerticalAlignment="Center" Margin="10" FontSize="20"></PasswordBox>
        <Button Name="loginButton" Click="loginButton_Click" Margin="10" FontSize="20"></Button>
        <Label Name="salidaLabel" FontSize="24" ></Label>
    </StackPanel>
</Window>
